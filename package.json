{
  "name": "mcmc",
  "version": "1.0.0",
  "description": "markov chain monte carlo",
  "homepage": "",
  "author": {
    "name": "Tianhao Yu",
    "email": "tyu14@u.rochester.edu",
    "url": ""
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "keywords": [
    ""
  ],
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^5.4.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-xo": "^0.24.2",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "istanbul": "^1.0.0-alpha.2",
    "jest": "^23.5.0",
    "lint-staged": "^7.2.2",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "prettier": "^1.11.1"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "mocha",
    "cover": "istanbul cover _mocha",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "repository": "git@github.com:tttianhao/mcmc.git",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT",
  "dependencies": {
    "ajv": "^6.5.4",
    "command-line-args": "^5.0.2",
    "jquery": "^3.3.1",
    "jsnetworkx": "^0.3.4"
  }
}
